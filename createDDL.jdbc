CREATE TABLE ROL (ID INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, NOMBRE VARCHAR(255) NOT NULL UNIQUE, SALARIO DOUBLE, PRIMARY KEY (ID))
CREATE TABLE EMPLEADO (ID INTEGER AUTO_INCREMENT NOT NULL, DNI VARCHAR(255) NOT NULL UNIQUE, ACTIVO TINYINT(1) default 0, APELLIDOS VARCHAR(255), CORREO VARCHAR(255) NOT NULL UNIQUE, NOMBRE VARCHAR(255), TELEFONO VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE TURNO (ID INTEGER AUTO_INCREMENT NOT NULL, FECHAHORAFIN DATETIME, FECHAHORAINICIO DATETIME, empleado_id INTEGER, rol_id INTEGER, PRIMARY KEY (ID))
CREATE TABLE DISPONIBILIDAD (ID INTEGER AUTO_INCREMENT NOT NULL, FECHAFIN DATETIME NOT NULL, FECHAINICIO DATETIME NOT NULL, EMPLEADO_ID INTEGER, PRIMARY KEY (ID))
ALTER TABLE TURNO ADD CONSTRAINT FK_TURNO_rol_id FOREIGN KEY (rol_id) REFERENCES ROL (ID)
ALTER TABLE TURNO ADD CONSTRAINT FK_TURNO_empleado_id FOREIGN KEY (empleado_id) REFERENCES EMPLEADO (ID)
ALTER TABLE DISPONIBILIDAD ADD CONSTRAINT FK_DISPONIBILIDAD_EMPLEADO_ID FOREIGN KEY (EMPLEADO_ID) REFERENCES EMPLEADO (ID)
